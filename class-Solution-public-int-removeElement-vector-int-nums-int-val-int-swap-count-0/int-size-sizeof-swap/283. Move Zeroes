class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int swap=0;
         for(int i=0;i<nums.size();i++){
        
        if(nums[i]==0){
            for(int j=i;j<nums.size();j++){
                nums[j]=0;
                swap=nums[j];
                nums[j]=nums[j+1];
                nums[j+1]=swap;
            }
         
        }
        
       
        
    }
    
    
            
            
        }
  };
