class Solution {
public:
    int minMoves(vector<int>& nums) {
        
        int minval=nums[0];
        if(nums.size()<=0){
            return 0;
        }
        else 
        int minval=nums[0];
        for(int i=0;i<nums.size();i++){
            
        
        if(nums[i]<minval){
            
            minval=nums[i];
        }
        }
        
        int temp=0;
        for(int i=0;i<nums.size();i++){
            
            temp= temp+(nums[i]-minval);
        }
        
        return temp;
    }
};
